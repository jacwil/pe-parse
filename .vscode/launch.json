    "version": "0.2.0",
    "configurations": [
        {
            "name": "pe-dump",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/dump-pe/Debug/dump-pe.exe",
            "args": ["c:\\windows\\system32\\windows.ui.xaml.controls.dll"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "logging": {
                "programOutput": true
            },
            "symbolSearchPath": "${workspaceFolder}/build/dump-pe/Debug",
            "preLaunchTask": "Build Debug"
        }
    ]
}